{
  "manifest_version": 3,
  "name": "GhostWriter",
  "version": "1.0",
  "description": "Auto-fill job applications from my MERN stack DB.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "http://localhost:5000/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "Images/icon.png"
  }
}